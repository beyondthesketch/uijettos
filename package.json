    "url": "https://github.com/beyondthesketch/uijettos/issues"
  },
  "homepage": "https://github.com/beyondthesketch/uijettos#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-cssresources": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/react": "^6.3.7",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^2.1.1",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.14.1",
