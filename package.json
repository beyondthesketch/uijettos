    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "uijettos.d.ts",
    "dist/",
